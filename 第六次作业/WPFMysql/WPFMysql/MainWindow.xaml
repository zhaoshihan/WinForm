<Window x:Class="WPFMysql.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFMysql"
        mc:Ignorable="d"
        Title="MainWindow" Height="360.548" Width="719.658">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="32,29,0,0" VerticalAlignment="Top" Width="154" SelectionChanged="comboBox_SelectionChanged"/>
        <Button x:Name="Modifybutton" Content="Modify" HorizontalAlignment="Left" Margin="377,29,0,0" VerticalAlignment="Top" Width="75" Click="Modifybutton_Click"/>
        <Button x:Name="Insertbutton" Content="Insert" HorizontalAlignment="Left" Margin="497,29,0,0" VerticalAlignment="Top" Width="75" Click="Insertbutton_Click"/>
        <Button x:Name="Deletebutton" Content="Delete" HorizontalAlignment="Left" Margin="616,29,0,0" VerticalAlignment="Top" Width="75" Click="Deletebutton_Click"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="32,88,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.501,0.479" Height="221" Width="659"/>

    </Grid>
</Window>
